{
  "name": "surveyapi",
  "version": "1.3.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main",
    "dev": "tsnd --respawn --transpile-only --inspect=0.0.0.0:9229 src/main",
    "start-docker2": "tsnd --poll --respawn --transpile-only --inspect=0.0.0.0:8222 ./dist/main",
    "start-docker": "nodemon -L --watch ./dist --inspect=0.0.0.0:8222 --nolazy ./dist/main",
    "start-docker3": "nodemon --inspect=0.0.0.0:8222 --legacy-watch ./dist/main",
    "debug": "ts-node-dev --respawn --transpile-only -r dotenv/config --inspect=0.0.0.0:9229 src/main",
    "up": "npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:staged": "npm t -- --findRelatedTests",
    "test:watch": "npm t -- --watch",
    "test:coverage": "npm t -- --coverage",
    "test:integration": "npm t -- -c ./jest.integration.config.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JeanFG86/SurveyApi.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JeanFG86/SurveyApi/issues"
  },
  "homepage": "https://github.com/JeanFG86/SurveyApi#readme",
  "devDependencies": {
    "@shelf/jest-mongodb": "^3.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^17.0.36",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.3",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-mock-extended": "^2.0.6",
    "lint-staged": "^12.5.0",
    "mockdate": "^3.0.5",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "^4.7.0",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "validator": "^13.7.0"
  },
  "engines": {
    "node": "16.x"
  }
}
